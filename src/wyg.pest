WHITESPACE = _{" "|"\t"}

str_inner = @{ char* }
char = {
	!("\"" | "\\") ~ ANY
    | "\\" ~ ("\"" | "\\" | "/" | "b" | "f" | "n" | "r" | "t" )
}

word = {(LETTER | NUMBER)+}
// words = ${word ~ (" " ~ word)*}

item = {("\"" ~ str_inner ~ "\"") | word}

field = { (item)+ }
record = { "Event:" ~ "\n" ~ field ~ "." ~ ("\n" ~ field ~ ".")* }
file = { SOI ~ (record ~ ("\r\n" | "\n"))* ~ EOI }
